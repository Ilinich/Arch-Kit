<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    >

    <Button
        android:id="@+id/btn_go_login_mvvm"
        style="@style/DefaultButtonStyle"
        android:layout_marginBottom="@dimen/layout_margin_32dp"
        android:text="@string/fragment_start__login_mvvm_btn"
        app:layout_constraintBottom_toTopOf="@+id/btn_go_login_mvi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <Button
        android:id="@+id/btn_go_login_mvi"
        style="@style/DefaultButtonStyle"
        android:layout_marginBottom="@dimen/layout_margin_32dp"
        android:text="@string/fragment_start__login_mvi_btn"
        app:layout_constraintBottom_toTopOf="@+id/btn_go_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <Button
        android:id="@+id/btn_go_profile"
        style="@style/DefaultButtonStyle"
        android:layout_marginBottom="@dimen/layout_margin_32dp"
        android:text="@string/fragment_start__go_profile_btn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>
